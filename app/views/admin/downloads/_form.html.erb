  <%= javascript_include_tag "/javascripts/ckeditor/ckeditor.js" %>
  <%= semantic_form_for [:admin, @download], :multipart => true do |f| %>
    <%= f.inputs :title %>
    <%= f.input :description, :input_html => { :class => "ckeditor" } %>
 <%= f.label :download_type %><br />
 <%= select :download, :download_type, ['iRAAS Journal', 'Telma 24 X 7', 'Slide Kits', 'Patient Education Material'] %><br />


  <%= f.input :attachment, :as => :file %>
    <%= f.buttons :commit %>
  <% end %>
